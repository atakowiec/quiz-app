{
  "name": "quiz-app",
  "version": "0.0.1",
  "description": "Quiz App With Ranked Mode and Abilities",
  "main": "index.js",
  "workspaces": [
    "frontend",
    "backend",
    "shared"
  ],
  "scripts": {
    "docker:compose": "docker-compose up -d",
    "frontend:dev": "npm run dev --workspace=frontend",
    "backend:dev": "npm run start:dev --workspace=backend",
    "start": "concurrently 'npm:backend:dev' 'npm:frontend:dev'",
    "stop": "docker-compose down"
  },
  "devDependencies": {
    "@types/webpack": "^5.28.0",
    "concurrently": "^7.0.0",
    "prettier": "^2.5.1",
    "wait-on": "^6.0.1"
  },
  "dependencies": {
    "@nestjs/platform-socket.io": "^10.4.2",
    "@nestjs/websockets": "^10.4.2",
    "pg": "^8.13.1",
    "react-h5-audio-player": "^3.9.3"
  }
}
